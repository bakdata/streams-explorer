{
  "incremental": true,
  "typescript": {
    "locked": true,
    "indentWidth": 2,
    "lineWidth": 80,
    "arguments.trailingCommas": "never",
    "parameters.trailingCommas": "never"
  },
  "json": {},
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json}"],
  "excludes": ["**/node_modules", "**/*-lock.json"],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.59.1.wasm",
    "https://plugins.dprint.dev/json-0.13.2.wasm"
  ]
}
